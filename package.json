{
  "name": "quizMasterTurbo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "db:deploy": "turbo run db:deploy",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "prettier": "^3.7.4"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@types/socket.io": "^3.0.1",
    "ioredis": "^5.9.2",
    "prisma": "6.0.0",
    "socket.io": "^4.8.2",
    "turbo": "^2.7.3",
    "typescript": "5.9.2"
  }
}
